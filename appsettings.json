{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"JWT": {
		"ValidIssuer": "https://auth.bcmtrac.com",
		"ValidAudience": "https://api.bcmtrac.com"
	},
	"Mfa": {
		"Issuer": "bcmtrac"
	},
	"Cors": {
		"AllowedOrigins": "https://auth.bcmtrac.com;https://app.bcmtrac.com;https://www.bcmtrac.com"
	},
	"PasswordHistory": { "KeepCount": 12, "ReuseWindowCount": 3, "MinAgeHours": 24 },
		"Email": {
			"EmailConfirm": { "Url": "https://auth.bcmtrac.com/email-confirm" }
		},
		"PasswordReset": { "Url": "https://auth.bcmtrac.com/reset-password" },
	"RefreshTokens": { "UseCookie": true },
	"Bridge": {
		"Enabled": false,
		"ApiKeyHeader": "X-Bridge-Api-Key",
		"ApiKey": "",
		"HeaderNames": ["X-Legacy-Session-1", "X-Legacy-Session-2", "X-Legacy-Session-3"],
		"JwtHeaderName": "X-Auth-JWT"
	},
		"Features": {
			"Swagger": false,
			"AutoMigrate": false,
			"Seed": false
		},
		"Throttle": { "Provider": "memory" },
		
}
